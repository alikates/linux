/dts-v1/;

#include "msm8953.dtsi"
#include "pm8953.dtsi"
#include "pmi8950.dtsi"

#include <dt-bindings/gpio/gpio.h>
#include <dt-bindings/interrupt-controller/irq.h>
#include <dt-bindings/leds/common.h>

/ {
	model = "Xiaomi Redmi 5 Plus";
	compatible = "xiaomi,vince", "qcom,msm8953";

	qcom,msm-id = <293 0>;
	qcom,board-id= <0x1000b 0x08>;

	chosen {
		#address-cells = <2>;
		#size-cells = <2>;
		ranges;
	};

	gpio-keys {
		compatible = "gpio-keys";

		pinctrl-names = "default";
		pinctrl-0 = <&gpio_key_default>;

		volume-up {
			label = "volume_up";
			linux,code = <KEY_VOLUMEUP>;
			gpios = <&tlmm 85 GPIO_ACTIVE_LOW>;
		};
	};

	qcom,battery-data {
		qcom,batt-id-range-pct = <5>;
		manufacturer = "coslight";
		model = "vince";

		qcom,max-voltage-uv = <4380000>;
		qcom,nom-batt-capacity-mah = <4000>;
		qcom,batt-id-kohm = <50>;
		qcom,battery-beta = <3435>;
		qcom,battery-type = "qrd_msm8953_coslight_4000mah";
		qcom,chg-rslow-comp-c1 = <2912209>;
		qcom,chg-rslow-comp-c2 = <3933866>;
		qcom,chg-rs-to-rslow = <758663>;
		qcom,chg-rslow-comp-thr = <0xa0>;
		qcom,checksum = <0xbfcd>;
		// qcom,thermal-coefficients = [c8 86 c1 50 d3 37];
		qcom,fg-profile-data = [ ED 83 4A 7D
					 73 81 EF 77
					 57 83 7C 44
					 E0 80 0E 8C
					 21 82 F5 99
					 D6 BC 4F C9
					 5A 0F FA 83
					 58 7D 65 81
					 AF 77 57 83
					 88 61 4C 81
					 D6 8D 76 82
					 15 98 FD B5
					 EA C1 52 13
					 AC 0F 4A 59
					 14 70 EE FC
					 BF 44 F4 3C
					 56 3A 00 00
					 D9 44 8F 39
					 55 3E 00 00
					 00 00 00 00
					 00 00 00 00
					 A7 6A B9 69
					 B4 6B 37 83
					 C7 6E AB 62
					 1D 70 67 81
					 F6 6C 71 60
					 16 7D 3E 92
					 1B 51 60 6E
					 5D A0 71 0C
					 28 00 FF 36
					 F0 11 30 03
					 00 00 00 0C ];
	};

	reserved-memory {
		/delete-node/ memory@85b00000;
		memory@84a00000 {
			reg = <0x0 0x84a00000 0x0 0x1900000>;
			no-map;
		};

		cont_splash_mem: memory@90001000 {
			reg = <0x0 0x90001000 0x0 (1080 * 2160 * 3)>;
			no-map;
		};

		/delete-node/ memory@8d600000;
		adsp_fw_mem: memory@8d600000 { // 0x8d600000 - 0x8e800000
			reg = <0x0 0x8d600000 0x0 0x1200000>;
			no-map;
		};

		/delete-node/ memory@8e700000;
		wcnss_fw_mem: memory@8e800000 { // 0x8e800000 - 0x8ef00000
			reg = <0x0 0x8e800000 0x0 0x700000>;
			no-map;
		};

		ramoops@9ff00000 {
			compatible = "ramoops";
			reg = <0x0 0x9ff00000 0x0 0x100000>;
			record-size = <0x1000>;
			console-size = <0x80000>;
			ftrace-size = <0x1000>;
			pmsg-size = <0x8000>;
		};
	};

	sensors: i2c@0 {
		compatible = "i2c-gpio";
		sda-gpios = <&tlmm 14 (GPIO_ACTIVE_HIGH|GPIO_OPEN_DRAIN)>;
		scl-gpios = <&tlmm 15 (GPIO_ACTIVE_HIGH|GPIO_OPEN_DRAIN)>;
		i2c-gpio,delay-us = <2>;        /* ~100 kHz */
		#address-cells = <1>;
		#size-cells = <0>;

		magnetometer@c {
			compatible = "asahi-kasei,ak09918";
			reg = <0x0c>;

			vdd-supply = <&pm8953_l6>;
			vid-supply = <&pm8953_l6>;

			mount-matrix = "1", "0", "0",
					"0", "1", "0",
					"0", "0", "1";
		};

		// light-sensor@53 {
		//     compatible = "liteon,ltr559";
		//     reg = <0x53>;
		//     // proximity-near-level = <75>;
		// };

		imu@6a {
			compatible = "st,lsm6dsl";
			reg = <0x6a>;
			vdd-supply = <&pm8953_l10>;
			vddio-supply = <&pm8953_l6>;
			mount-matrix = "-1", "0", "0",
					"0", "-1", "0",
					"0", "0", "1";
		};
	};

	vph_pwr: vph-pwr-regulator {
		compatible = "regulator-fixed";
		regulator-name = "vph_pwr";
		regulator-always-on;
		regulator-boot-on;
	};
};

&dsi0 {
	vdda-supply = <&pm8953_s3>;
	vddio-supply = <&pm8953_l6>;

	panel: panel@0 {
		compatible = "xiaomi,vince-panel";
		reg = <0>;

		backlight = <&pmi8950_wled>;

		reset-gpios = <&tlmm 61 GPIO_ACTIVE_LOW>;

		pinctrl-names = "default", "sleep";
		pinctrl-0 = <&pmx_mdss_default &mdss_te_default>;
		pinctrl-1 = <&pmx_mdss_sleep &mdss_te_default>;

		vsp-supply = <&lab_vreg>;
		vsn-supply = <&ibb_vreg>;

		port {
			panel_in: endpoint {
				remote-endpoint = <&dsi0_out>;
			};
		};
	};
};

&dsi0_out {
	data-lanes = <0 1 2 3>;
	remote-endpoint = <&panel_in>;
};

&dsi_phy0 {
	vcca-supply = <&pm8953_l3>;
};

&hsusb_phy {
	status = "okay";

	vdd-supply = <&pm8953_l3>;
	vdda-pll-supply = <&pm8953_l7>;
	vdda-phy-dpdm-supply = <&pm8953_l13>;
};

&i2c_2 {
	status = "okay";

	// speaker_codec: tas2557@4c {
